{
  "kind": "collectionType",
  "collectionName": "module_courses",
  "info": {
    "singularName": "module-course",
    "pluralName": "module-courses",
    "displayName": "ModuleCourse",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "module_course"
    },
    "author": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description_title": {
      "type": "text"
    },
    "description_subtitle": {
      "type": "string"
    },
    "description_module": {
      "type": "text"
    },
    "time_duration": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "course_level": {
      "type": "enumeration",
      "enum": ["Principiante", "Intermedio", "Avanzado"],
      "required": true
    },
    "keywords": {
      "type": "json"
    },
    "duration_type": {
      "type": "enumeration",
      "enum": ["minutos", "horas", "días", "semanas", "meses", "años"]
    }
  }
}
